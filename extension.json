{
	"name": "SphinxSearch",
	"author": "Svemir Brkic, Paul Grinberg",
	"url": "https://www.mediawiki.org/wiki/Extension:SphinxSearch",
	"descriptionmsg": "sphinxsearch-desc",
	"license-name": "GPL-2.0",
	"type": "other",
	"ConfigRegistry": {
	},
	"ExtensionFunctions": [
        "SphinxSearchInit::initialize"
	],
	"AutoloadClasses": {
        "SphinxSearchInit": "SphinxSearch.php",
		"SphinxMWSearch": "SphinxMWSearch.php",
		"SphinxMWSearchResult": "SphinxMWSearch.php",
		"SphinxMWSearchResultSet": "SphinxMWSearch.php"
	},
    "ExtensionMessagesFiles": {
        "SphinxSearch": "SphinxSearch.i18n.php"
	},
	"MessagesDirs": {
		"SphinxSearch": [
			"i18n"
		]
    },
	"ResourceModules": {
	},
	"ResourceFileModulePaths": {
	},
	"Hooks": {
	},
	"config": {
        "SphinxSearch_host": "127.0.0.1",
        "SphinxSearch_port": 9312,
        "SphinxSearch_index": "wiki_main",
        "SphinxSearch_index_list": "*",
        "SphinxSearch_index_weights": "null",
        "SphinxSearch_mode": "SPH_MATCH_EXTENDED2",
        "SphinxSearch_sortmode": "SPH_SORT_RELEVANCE",
        "SphinxSearch_sortby": "",
        "SphinxSearch_maxmatches": 1000,
        "SphinxSearch_cutoff": 0,
        "SphinxSearch_weights": {
            "old_text": 1,
            "page_title": 100
        },
        "SphinxSearchMWHighlighter": false,
        "SphinxSuggestMode": "",
        "SphinxSearchAspellPath": "aspell",
        "SphinxSearchPersonalDictionary": "",
        "EnableSphinxPrefixSearch": false
	},
	"manifest_version": 1
}
